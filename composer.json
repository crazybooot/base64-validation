{
  "name": "crazybooot/base64-validation",
  "description": "Laravel validators for base64 encoded files",
  "keywords": [
    "base64",
    "validation",
    "files",
    "encoded",
    "images",
    "download",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "crazybooot",
      "email": "crazybooot@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "laravel/framework": "^5.5|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8",
    "squizlabs/php_codesniffer": "^3.5",
    "intervention/image": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Crazybooot\\Base64Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Crazybooot\\Base64Validation\\Providers\\ServiceProvider"
      ]
    }
  }
}
