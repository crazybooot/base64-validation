{
    "name": "crazybooot/base64-validation",
    "description": "Laravel validators for base64 encoded files",
    "keywords": [
        "base64",
        "validation",
        "files",
        "encoded",
        "images",
        "download",
        "laravel"
    ],
    "authors": [
        {
            "name": "crazybooot",
            "email": "crazybooot@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1|^8.0",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Crazybooot\\Base64Validation\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crazybooot\\Base64Validation\\Providers\\ServiceProvider"
            ]
        }
    }
}
